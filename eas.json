{
  "cli": {
    "version": ">= 10.2.4",
    "appVersionSource": "remote"
  },
  "build": {
    "release": {
      "distribution": "store",
      "ios": {
        "credentialsSource": "local"
      },
      "env": {
        "APP_VARIANT": "production"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "credentialsSource": "local",
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development"
      },
      "android": {
        "credentialsSource": "remote",
        "env": {
          "GOOGLE_SERVICES_JSON": "@google-service-json"
        }
      }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "channel": "staging",
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "channel": "production",
      "ios": {
        "credentialsSource": "remote"
      },
      "android": {
        "credentialsSource": "remote",
        "env": {
          "GOOGLE_SERVICES_JSON": "@google-service-json"
        }
      }
    },
    "my-profile": {
      "environment": "production",
      "env": {
        "APP_VARIANT": "my-profile"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6742695484"
      }
    }
  }
}
