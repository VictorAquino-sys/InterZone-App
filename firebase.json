{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "react-native": {
    "crashlytics_debug_enabled": true,
    "crashlytics_javascript_exception_handler_chaining_enabled": false,
    "crashlytics_is_error_generation_on_js_crash_enabled": true
  }
}
